<nav class="navbar navbar-expand-sm ">
    <div class="container-fluid">
      <a class="navbar-brand" href="/post/list">Seolgu</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" 
              aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="/post/write">글쓰기</a>
          </li>
          <% if (user) { %>
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/auth/mypage">마이페이지</a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/auth/logout">로그아웃</a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/chat/list">채팅</a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/auth/mypage"><%= user.username %></a>
            </li>
          <% } else { %>
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/auth/login">로그인</a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/auth/kakao">카카오로그인</a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/auth/register">회원가입</a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/chat/list">채팅</a>
            </li>
          <% } %>
        </ul>
        <form class="d-flex" action ="/search" role="search" method="post">
            <input class="form-control me-2" name="val" type="search" placeholder="Search" aria-label="Search">
            <button class="btn btn-outline-success" type="submit">Search</button>
          </form>
      </div>
    </div>
  </nav>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
